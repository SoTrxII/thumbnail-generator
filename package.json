{
  "name": "thumbnail-generator",
  "version": "1.0.0",
  "author": "Sotrx",
  "license": "MIT",
  "scripts": {
    "pretest": "./node_modules/.bin/jest --clearCache",
    "test": "./node_modules/.bin/jest",
    "prebuild": "npx shx rm -rf dist/*",
    "build": "tsc && npx shx cp package.json dist/package.json ",
    "build:f": "npm run build && faas-cli build -f thumbnail-generator.yml --shrinkwrap && npx shx cp -r package.json build/thumbnail-generator/function/ && npx shx cp -r dist build/thumbnail-generator/function/core && npx shx cp -r assets build/thumbnail-generator/function",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@fluffy-spoon/substitute": "^1.208.0",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/ioredis": "^4.26.1",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fontkit": "^1.8.1",
    "http-status-codes": "^2.2.0",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.2",
    "inversify": "^5.1.1",
    "inversify-inject-decorators": "^3.1.0",
    "ioredis": "^4.27.1",
    "jsonschema": "^1.4.0",
    "node-fetch": "^2.6.1",
    "node-vibrant": "^3.2.1-alpha.1",
    "pino": "^6.11.3",
    "pino-http": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/fontkit": "^1.8.0",
    "@types/gm": "^1.18.9",
    "@types/http-status-codes": "^1.2.0",
    "@types/imagemin": "^7.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/node-fetch": "^2.5.10",
    "@types/pino": "^6.3.8",
    "@types/pino-http": "^5.4.1",
    "@types/sharp": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.2.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ]
  },
  "imports": {
    "#core/*": "./core/*"
  }
}
